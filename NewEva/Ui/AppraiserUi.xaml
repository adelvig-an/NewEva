                                        VerticalAlignment="Center"
                                        FontFamily="TT Norms Pro Bold"
                                        FontSize="20"
                                        Text="Тут будет загруженная картинка"
                                        TextAlignment="Center"
                                        TextWrapping="Wrap" />
                                </Border>
                                <Border Grid.Row="1" Margin="16,4,16,0">
                                    <DockPanel HorizontalAlignment="Center">
                                        <Button
                                            Grid.Row="1"
                                            Margin="3,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Content="Выбрать"
                                            FontFamily="TT Norms Pro Bold"
                                            FontSize="16" />
                                        <Button
                                            Grid.Row="1"
                                            Margin="3,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Background="Red"
                                            BorderBrush="Red"
                                            Content="Удалить"
                                            FontFamily="TT Norms Pro Bold"
                                            FontSize="16" />
                                    </DockPanel>
                                </Border>
                            </Grid>
                        </Border>
                    </Grid>
                </Border>
            </Grid>
        </Border>
        <Border Grid.Row="3">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Margin="16">
                    <TextBlock
                        FontFamily="TT Norms Medium"
                        FontSize="14"
                        Text="Страхование ответственности" />
                </Border>
                <Border Grid.Row="1">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>


                        <Border
                            Grid.Row="0"
                            Grid.RowSpan="4"
                            Grid.Column="2"
                            Margin="0,16">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Border
                                    Grid.Row="0"
                                    Margin="16,0,16,4"
                                    BorderBrush="Black"
                                    BorderThickness="1">
                                    <TextBlock
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontFamily="TT Norms Pro Bold"
                                        FontSize="20"
                                        Text="Тут будет загруженная картинка"
                                        TextAlignment="Center"
                                        TextWrapping="Wrap" />
                                </Border>
                                <Border Grid.Row="1" Margin="16,4,16,0">
                                    <DockPanel HorizontalAlignment="Center">
                                        <Button
                                            Grid.Row="1"
                                            Margin="3,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Content="Выбрать"
                                            FontFamily="TT Norms Pro Bold"
                                            FontSize="16" />
                                        <Button
                                            Grid.Row="1"
                                            Margin="3,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Background="Red"
                                            BorderBrush="Red"
                                            Content="Удалить"
                                            FontFamily="TT Norms Pro Bold"
                                            FontSize="16" />
                                    </DockPanel>
                                </Border>
                            </Grid>
                        </Border>
                    </Grid>
                </Border>
            </Grid>
        </Border>
    </Grid>


